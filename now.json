{
    "version": 2,
    "name": "shawb-wong",
    "alias": "shawb-wong",
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build",
        "config": { "distDir": "public" }
      }
    ],
    "build": {
      "env": {
        "API_BASE_URL": "@api_base_url",
        "API_SPACE_ID": "@api_space_id",
        "API_TOKEN": "@api_token",
        "HOST_SERVER": "now",
        "GITALK_ID": "@gitalk_id",
        "GITALK_SECRET": "@gitalk_secret"
      }
    }
  }